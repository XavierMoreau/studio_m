{% extends 'base.html.twig' %}

{% block body %}
    <h1>Scriptquestion</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ scriptQuestion.id }}</td>
            </tr>
            <tr>
                <th>Question</th>
                <td>{{ scriptQuestion.question }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('scriptquestion_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('scriptquestion_edit', { 'id': scriptQuestion.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
